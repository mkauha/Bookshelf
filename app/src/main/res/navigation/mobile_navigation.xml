<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="fi.mkauha.bookshelf.ui.mybooksview.MyBooksFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_mybooks" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="fi.mkauha.bookshelf.ui.wishlistview.WishListFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_wishlist" />

    <fragment
        android:id="@+id/navigation_libraries"
        android:name="fi.mkauha.bookshelf.ui.librariesview.LibrariesFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_libraries" />
</navigation>